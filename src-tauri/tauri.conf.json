{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Notebook Viewer",
  "version": "0.1.0",
  "identifier": "build.conductor.notebook-viewer",
  "build": {
    "frontendDist": "../src",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "title": "Notebook Viewer",
        "width": 1200,
        "height": 800,
        "visible": false,
        "resizable": true
      }
    ]
  }
}
